{
  "manifest_version": 2,
  "name": "Responsible",
  "description": "Responsibility is hard to teach. On the internet, we can do it for you.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "notifications",
    "storage",
    "*://*/*"
  ]
}